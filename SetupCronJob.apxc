// Schedule the batch to run every 5 minutes
String className = 'BatchSendAccountsEvonaScheduler';
for (Integer i = 0; i < 60; i += 5) {
    String cronExp = '0 ' + i + ' * * * ?';
    String jobName = 'evona-send-account-updates-' + i;
    System.schedule(jobName, cronExp, new BatchSendAccountsEvonaScheduler());
}
